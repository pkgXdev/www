    "ghc-{{version}}",
    "ghc-pkg",
    "ghc-pkg-{{version.marketing}}",
    "ghc-pkg-{{version}}",
    "ghci",
    "ghci-{{version.marketing}}",
    "ghci-{{version}}",
    "ghcup",
    "haddock",
    "haddock-{{version.marketing}}",
    "haddock-{{version}}",
    "hp2ps",
    "hp2ps-{{version.marketing}}",
    "hp2ps-{{version}}",
    "hpc",
    "hpc-{{version.marketing}}",
    "hpc-{{version}}",
    "hsc2hs",
    "hsc2hs-{{version.marketing}}",
    "hsc2hs-{{version}}",
    "runghc",
    "runghc-{{version.marketing}}",
    "runghc-{{version}}",
    "runhaskell",
    "runhaskell-{{version.marketing}}",
    "runhaskell-{{version}}"
